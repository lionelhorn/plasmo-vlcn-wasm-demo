{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "next-env.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "preserve",
    "paths": {
      "~*": [
        "./src/*"
      ]
    },
    "baseUrl": ".",
    "isolatedModules": true
  }
}
